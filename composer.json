{
  "name": "vilnacrm/user-service",
  "description": "Microservices template for modern PHP applications",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-xml": "*",
    "api-platform/core": "^4.1",
    "aws/aws-sdk-php": "^3.343",
    "cfpinto/graphql": "^2.0",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.3",
    "dpauli/graphql-request-builder": "^1.8",
    "fakerphp/faker": "^1.23.0",
    "league/oauth2-server-bundle": "^1.0",
    "lexik/jwt-authentication-bundle": "^3.0.0",
    "nelmio/cors-bundle": "^2.4",
    "phpmailer/phpmailer": "^6.6",
    "ramsey/uuid": "^4.7",
    "runtime/frankenphp-symfony": "^0.2",
    "symfony/amazon-sqs-messenger": "7.3.*",
    "symfony/asset": "7.3.*",
    "symfony/cache": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/doctrine-messenger": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/flex": "^2.7",
    "symfony/framework-bundle": "7.3.*",
    "symfony/mailer": "7.3.*",
    "symfony/messenger": "7.3.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/password-hasher": "7.3.*",
    "symfony/proxy-manager-bridge": "^6.4",
    "symfony/rate-limiter": "7.3.*",
    "symfony/redis-messenger": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/security-http": "^7.3",
    "symfony/string": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/twig-bridge": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/uid": "7.3.*",
    "symfony/validator": "7.3.*",
    "symfony/yaml": "7.3.*",
    "thecodingmachine/safe": "^3.0",
    "twig/extra-bundle": "^3.10",
    "twig/inky-extra": "^3.8",
    "webonyx/graphql-php": "^15.17"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": false
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts",
      "php bin/console lexik:jwt:generate-keypair --skip-if-exists"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.3.*",
      "docker": true
    }
  },
  "require-dev": {
    "behat/behat": "^3.12",
    "behat/gherkin": "4.12.0",
    "dama/doctrine-test-bundle": "^8.2",
    "dg/bypass-finals": "^1.5",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "friends-of-behat/symfony-extension": "^2.6",
    "friendsofphp/php-cs-fixer": "^3.14",
    "infection/infection": "dev-master",
    "marcocesarato/php-conventional-changelog": "^1.16",
    "nunomaduro/phpinsights": "dev-master",
    "pheromone/phpcs-security-audit": "^2.0",
    "phpmd/phpmd": "^2.15",
    "phpmetrics/phpmetrics": "^2.8",
    "phpunit/phpunit": "^10.5",
    "psalm/plugin-phpunit": "dev-master",
    "qossmic/deptrac-shim": "^1.0",
    "justinrainbow/json-schema": "^6.6",
    "symfony/browser-kit": "7.3.*",
    "symfony/css-selector": "7.3.*",
    "symfony/maker-bundle": "^1.47",
    "symfony/phpunit-bridge": "7.3.*",
    "symfony/stopwatch": "7.3.*",
    "symfony/web-profiler-bundle": "7.3.*",
    "vimeo/psalm": "^6.13",
    "zenstruck/messenger-test": "^1.9"
  }
}
