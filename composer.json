{
    "name": "vilnacrm/php-serice-template",
    "description": "Microservices template for modern PHP applications",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "api-platform/core": "^3.0",
        "cfpinto/graphql": "^2.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "dpauli/graphql-request-builder": "^1.8",
        "fakerphp/faker": "^1.23.0",
        "lambdish/phunctional": "^2.1",
        "league/oauth2-server-bundle": "^0.5.0",
        "lexik/jwt-authentication-bundle": "^2.20.3",
        "nelmio/cors-bundle": "^2.4",
        "phpmailer/phpmailer": "^6.6",
        "ramsey/uuid": "4.7.5",
        "sebastian/phpcpd": "^6.0",
        "symfony/amazon-sqs-messenger": "6.3.*",
        "symfony/asset": "^6.3",
        "symfony/cache": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/messenger": "6.3.*",
        "symfony/monolog-bundle": "*",
        "symfony/password-hasher": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/security-http": "^6.3.8",
        "symfony/translation": "6.3.*",
        "symfony/twig-bridge": "^6.3.8",
        "symfony/twig-bundle": "^6.3",
        "symfony/uid": "6.3.*",
        "symfony/validator": "^6.3",
        "symfony/yaml": "6.3.*",
        "thecodingmachine/safe": "^2.5",
        "twig/extra-bundle": "^3.8",
        "twig/inky-extra": "^3.8",
        "webonyx/graphql-php": "^14.11"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "captainhook/plugin-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "php bin/console lexik:jwt:generate-keypair --skip-if-exists"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-autoload-dump": "@./vendor/bin/captainhook install -f -s"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*",
            "docker": true
        }
    },
    "require-dev": {
        "behat/behat": "^3.12",
        "captainhook/captainhook": "^5.11",
        "captainhook/plugin-composer": "^5.3",
        "dg/bypass-finals": "^1.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friends-of-behat/symfony-extension": "^2.4",
        "friendsofphp/php-cs-fixer": "^3.14",
        "marcocesarato/php-conventional-changelog": "^1.16",
        "nunomaduro/phpinsights": "^2.7",
        "pheromone/phpcs-security-audit": "^2.0",
        "phpmetrics/phpmetrics": "^2.8",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18.4",
        "psalm/plugin-symfony": "^5.0",
        "ramsey/conventional-commits": "^1.3",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.47",
        "symfony/phpunit-bridge": "^6.2",
        "vimeo/psalm": "^5.1",
        "weirdan/doctrine-psalm-plugin": "^2.5",
        "zenstruck/messenger-test": "^1.9.1"
    },
    "version": "1.1.0"
}
