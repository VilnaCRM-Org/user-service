{
  "name": "Load",
  "version": "1.0.0",
  "description": "Load testing for VilnaCRM user-service",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare-users": "cd ../../.. && docker run -v ./tests/Load:/loadTests --net=host --rm --user $(id -u) k6 run /loadTests/utils/prepareUsers.js",
    "smoke-test": "cd ../../.. && ./tests/Load/run-smoke-load-tests.sh",
    "average-test": "cd ../../.. && ./tests/Load/run-average-load-tests.sh",
    "stress-test": "cd ../../.. && ./tests/Load/run-stress-load-tests.sh",
    "spike-test": "cd ../../.. && ./tests/Load/run-spike-load-tests.sh",
    "load-test": "cd ../../.. && ./tests/Load/run-load-tests.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0"
  },
  "keywords": ["load-testing", "k6", "performance"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "acorn": "^8.14.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.15.0"
    "@eslint/js": "^9.28.0",
    "eslint": "^9.28.0",
    "globals": "^16.2.0"
  }
}
