{
  "name": "frontend-ssr-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-export-optimize-images",
    "build:analyze": "ANALYZE=true pnpm run build",
    "start": "next build && next-export-optimize-images && npx serve@latest out",
    "git-hooks-install": "husky install",
    "storybook-start": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc --newLine LF",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "pnpm exec playwright test ./src/test/e2e",
    "test:mutation": "npx stryker run",
    "test:visual": "pnpm exec playwright test ./src/test/visual",
    "test:unit": "jest --verbose",
    "test:memory-leak": "node ./src/test/memory-leak/runMemlabTests",
    "lighthouse:desktop": "next build && next-export-optimize-images && lhci autorun --config=lighthouserc.desktop.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:mobile": "next build && next-export-optimize-images && lhci autorun --config=lighthouserc.mobile.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:desktop-autorun": "lhci autorun --config=lighthouserc.desktop.js",
    "lighthouse:mobile-autorun": "lhci autorun --config=lighthouserc.mobile.js",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.13",
    "@mui/system": "^5.15.13",
    "@next/bundle-analyzer": "^14.2.3",
    "@next/env": "^14.1.4",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "i18next": "^23.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-i18next": "^14.1.0",
    "swagger-ui-react": "^5.17.14",
    "terser-webpack-plugin": "^5.3.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@faker-js/faker": "^8.4.1",
    "@lhci/cli": "^0.13.0",
    "@memlab/api": "^1.0.29",
    "@memlab/heap-analysis": "^1.0.27",
    "@microsoft/api-extractor": "^7.39.4",
    "@next/eslint-plugin-next": "^14.1.0",
    "@next/third-parties": "^14.1.4",
    "@playwright/test": "^1.44.1",
    "@sentry/node": "^7.98.0",
    "@sentry/react": "^7.98.0",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@storybook/testing-library": "0.0.13",
    "@stryker-mutator/core": "^8.2.6",
    "@stryker-mutator/jest-runner": "^8.2.6",
    "@stryker-mutator/typescript": "^4.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/material-ui": "^0.21.16",
    "@types/node": "18.11.9",
    "@types/react": "18.2.40",
    "@types/react-dom": "^18.2.18",
    "@types/swagger-ui-react": "^4.18.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "markdownlint-cli": "^0.39.0",
    "memlab": "^1.1.43",
    "next": "14.1.4",
    "next-export-optimize-images": "^4.5.0",
    "prettier": "^3.2.4",
    "sass": "^1.77.6",
    "semver": "^7.5.4",
    "storybook": "^8.0.0",
    "swiper": "^11.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "web-vitals": "^3.5.2",
    "webpack": "^5.90.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
