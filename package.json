    "next": "14.1.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^14.0.1",
    "react-swipeable-views": "^0.14.0",
