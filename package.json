{
  "name": "frontend-ssr-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "git-hooks-install": "husky install",
    "storybook-start": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc --newLine LF",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "playwright run --headless --browser chrome",
    "test:e2e-local": "playwright open",
    "test:unit": "jest --verbose",
    "test:memory-leak": "memlab run -v --scenario ./memlab/memoryLeakTest.js",
    "lighthouse:desktop": "next build && lhci autorun --config=lighthouserc.desktop.js",
    "lighthouse:mobile": "next build && lhci autorun --config=lighthouserc.mobile.js",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.9.7",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.13",
    "@mui/system": "^5.15.13",
    "graphql": "^16.8.1",
    "i18next": "^23.10.1",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-i18next": "^14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@faker-js/faker": "^8.4.1",
    "@lhci/cli": "^0.13.0",
    "@microsoft/api-extractor": "^7.42.3",
    "@next/bundle-analyzer": "^14.1.3",
    "@next/eslint-plugin-next": "^14.1.3",
    "@playwright/test": "^1.42.1",
    "@sentry/node": "^7.107.0",
    "@sentry/react": "^7.107.0",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/material-ui": "^0.21.17",
    "@types/node": "18.11.9",
    "@types/react": "18.2.40",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "markdownlint-cli": "^0.39.0",
    "memlab": "^1.1.45",
    "prettier": "^3.2.5",
    "semver": "^7.6.0",
    "storybook": "^8.0.0",
    "swiper": "^11.0.7",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "web-vitals": "^3.5.2",
    "webpack": "^5.90.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
