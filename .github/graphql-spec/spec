input updateConfirmPasswordResetDtoInput {
  id: ID!
  clientMutationId: String
}

"Updates a ConfirmPasswordResetDto."
type updateConfirmPasswordResetDtoPayload {
  confirmPasswordResetDto: ConfirmPasswordResetDto
  clientMutationId: String
}

"Deletes a ConfirmPasswordResetDto."
input deleteConfirmPasswordResetDtoInput {
  id: ID!
  clientMutationId: String
}

"Deletes a ConfirmPasswordResetDto."
type deleteConfirmPasswordResetDtoPayload {
  confirmPasswordResetDto: ConfirmPasswordResetDto
