input updateRequestPasswordResetDtoInput {
  id: ID!
  clientMutationId: String
}

"Updates a RequestPasswordResetDto."
type updateRequestPasswordResetDtoPayload {
  requestPasswordResetDto: RequestPasswordResetDto
  clientMutationId: String
}

"Deletes a RequestPasswordResetDto."
input deleteRequestPasswordResetDtoInput {
  id: ID!
  clientMutationId: String
}

"Deletes a RequestPasswordResetDto."
type deleteRequestPasswordResetDtoPayload {
  requestPasswordResetDto: RequestPasswordResetDto
  clientMutationId: String
}

