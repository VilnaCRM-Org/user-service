type Query {
  node(id: ID!): Node
  user(id: ID!): User
  users(
    "Returns the first n elements from the list."
    first: Int

    "Returns the last n elements from the list."
    last: Int

    "Returns the elements in the list that come before the specified cursor."
    before: String

    "Returns the elements in the list that come after the specified cursor."
    after: String
  ): UserCursorConnection
  requestPasswordResetDto(id: ID!): RequestPasswordResetDto
  requestPasswordResetDtos(
    "Returns the first n elements from the list."
    first: Int

    "Returns the last n elements from the list."
    last: Int

    "Returns the elements in the list that come before the specified cursor."
    before: String

    "Returns the elements in the list that come after the specified cursor."
    after: String
  ): RequestPasswordResetDtoCursorConnection
  confirmPasswordResetDto(id: ID!): ConfirmPasswordResetDto
  confirmPasswordResetDtos(
    "Returns the first n elements from the list."
    first: Int

    "Returns the last n elements from the list."
    last: Int

    "Returns the elements in the list that come before the specified cursor."
    before: String

    "Returns the elements in the list that come after the specified cursor."
    after: String
  ): ConfirmPasswordResetDtoCursorConnection
  healthCheck(id: ID!): HealthCheck
  healthChecks(
    "Returns the first n elements from the list."
    first: Int

    "Returns the last n elements from the list."
    last: Int

    "Returns the elements in the list that come before the specified cursor."
    before: String

    "Returns the elements in the list that come after the specified cursor."
    after: String
  ): HealthCheckCursorConnection
}

"A node, according to the Relay specification."
interface Node {
  "The id of this node."
  id: ID!
}

type User implements Node {
  id: ID!
  confirmed: Boolean!
  email: String!
  initials: String!
}

"Cursor connection for User."
type UserCursorConnection {
  edges: [UserEdge]
  pageInfo: UserPageInfo!
  totalCount: Int!
}

"Edge of User."
type UserEdge {
  node: User
  cursor: String!
}

"Information about the current page."
type UserPageInfo {
  endCursor: String
  startCursor: String
  hasNextPage: Boolean!
  hasPreviousPage: Boolean!
}

type RequestPasswordResetDto implements Node {
  id: ID!
  email: String
}

"Cursor connection for RequestPasswordResetDto."
type RequestPasswordResetDtoCursorConnection {
  edges: [RequestPasswordResetDtoEdge]
  pageInfo: RequestPasswordResetDtoPageInfo!
  totalCount: Int!
}

"Edge of RequestPasswordResetDto."
type RequestPasswordResetDtoEdge {
  node: RequestPasswordResetDto
  cursor: String!
}

"Information about the current page."
type RequestPasswordResetDtoPageInfo {
  endCursor: String
  startCursor: String
  hasNextPage: Boolean!
  hasPreviousPage: Boolean!
}

type ConfirmPasswordResetDto implements Node {
  id: ID!
  token: String
  newPassword: String
}

"Cursor connection for ConfirmPasswordResetDto."
type ConfirmPasswordResetDtoCursorConnection {
  edges: [ConfirmPasswordResetDtoEdge]
  pageInfo: ConfirmPasswordResetDtoPageInfo!
  totalCount: Int!
}

"Edge of ConfirmPasswordResetDto."
type ConfirmPasswordResetDtoEdge {
  node: ConfirmPasswordResetDto
  cursor: String!
}

"Information about the current page."
type ConfirmPasswordResetDtoPageInfo {
  endCursor: String
  startCursor: String
  hasNextPage: Boolean!
  hasPreviousPage: Boolean!
}

type HealthCheck implements Node {
  id: ID!
}

"Cursor connection for HealthCheck."
type HealthCheckCursorConnection {
  edges: [HealthCheckEdge]
  pageInfo: HealthCheckPageInfo!
  totalCount: Int!
}

"Edge of HealthCheck."
type HealthCheckEdge {
  node: HealthCheck
  cursor: String!
}

"Information about the current page."
type HealthCheckPageInfo {
  endCursor: String
  startCursor: String
  hasNextPage: Boolean!
  hasPreviousPage: Boolean!
}

type Mutation {
  "Deletes a User."
  deleteUser(input: deleteUserInput!): deleteUserPayload

  "Confirms a User."
  confirmUser(input: confirmUserInput!): confirmUserPayload

  "Creates a User."
  createUser(input: createUserInput!): createUserPayload

  "Updates a User."
  updateUser(input: updateUserInput!): updateUserPayload

  "ResendEmailTos a User."
  resendEmailToUser(input: resendEmailToUserInput!): resendEmailToUserPayload

  "RequestPasswordResets a User."
  requestPasswordResetUser(input: requestPasswordResetUserInput!): requestPasswordResetUserPayload

  "ConfirmPasswordResets a User."
  confirmPasswordResetUser(input: confirmPasswordResetUserInput!): confirmPasswordResetUserPayload

  "Updates a RequestPasswordResetDto."
  updateRequestPasswordResetDto(input: updateRequestPasswordResetDtoInput!): updateRequestPasswordResetDtoPayload

  "Deletes a RequestPasswordResetDto."
  deleteRequestPasswordResetDto(input: deleteRequestPasswordResetDtoInput!): deleteRequestPasswordResetDtoPayload

  "Creates a RequestPasswordResetDto."
  createRequestPasswordResetDto(input: createRequestPasswordResetDtoInput!): createRequestPasswordResetDtoPayload

  "Updates a ConfirmPasswordResetDto."
  updateConfirmPasswordResetDto(input: updateConfirmPasswordResetDtoInput!): updateConfirmPasswordResetDtoPayload

  "Deletes a ConfirmPasswordResetDto."
  deleteConfirmPasswordResetDto(input: deleteConfirmPasswordResetDtoInput!): deleteConfirmPasswordResetDtoPayload

  "Creates a ConfirmPasswordResetDto."
  createConfirmPasswordResetDto(input: createConfirmPasswordResetDtoInput!): createConfirmPasswordResetDtoPayload

  "Updates a HealthCheck."
  updateHealthCheck(input: updateHealthCheckInput!): updateHealthCheckPayload

  "Deletes a HealthCheck."
  deleteHealthCheck(input: deleteHealthCheckInput!): deleteHealthCheckPayload

  "Creates a HealthCheck."
  createHealthCheck(input: createHealthCheckInput!): createHealthCheckPayload
}

"Deletes a User."
input deleteUserInput {
  id: ID!
  clientMutationId: String
}

"Deletes a User."
type deleteUserPayload {
  user: deleteUserPayloadData
  clientMutationId: String
}

"Deletes a User."
type deleteUserPayloadData implements Node {
  id: ID!
}

"Confirms a User."
input confirmUserInput {
  clientMutationId: String
  token: String!
}

"Confirms a User."
type confirmUserPayload {
  user: User
  clientMutationId: String
}

"Creates a User."
input createUserInput {
  clientMutationId: String
  email: String!
  initials: String!
  password: String!
}

"Creates a User."
type createUserPayload {
  user: User
  clientMutationId: String
}

"Updates a User."
input updateUserInput {
  clientMutationId: String
  id: ID!
  newPassword: String
  password: String!
  email: String
  initials: String
}

"Updates a User."
type updateUserPayload {
  user: User
  clientMutationId: String
}

"ResendEmailTos a User."
input resendEmailToUserInput {
  clientMutationId: String
  id: ID!
}

"ResendEmailTos a User."
type resendEmailToUserPayload {
  user: User
  clientMutationId: String
}

"RequestPasswordResets a User."
input requestPasswordResetUserInput {
  clientMutationId: String
  email: String!
}

"RequestPasswordResets a User."
type requestPasswordResetUserPayload {
  user: User
  clientMutationId: String
}

"ConfirmPasswordResets a User."
input confirmPasswordResetUserInput {
  clientMutationId: String
  token: String!
  newPassword: String!
}

"ConfirmPasswordResets a User."
type confirmPasswordResetUserPayload {
  user: User
  clientMutationId: String
}

