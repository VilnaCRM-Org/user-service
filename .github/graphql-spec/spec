input createRequestPasswordResetDtoInput {
  clientMutationId: String
}

"Creates a RequestPasswordResetDto."
type createRequestPasswordResetDtoPayload {
  requestPasswordResetDto: RequestPasswordResetDto
  clientMutationId: String
}

